    "db:create": "node -r ts-node/register ./node_modules/.bin/sequelize-cli db:create",
    "db:migrate": "node -r ts-node/register ./node_modules/.bin/sequelize-cli db:migrate",
    "db:seed": "node -r ts-node/register ./node_modules/.bin/sequelize-cli db:seed:all"
